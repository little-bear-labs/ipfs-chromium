OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/doc/
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@ @CMAKE_SOURCE_DIR@/README.md @CMAKE_SOURCE_DIR@/doc
GENERATE_LATEX = NO
GENERATE_MAN = YES
RECURSIVE = YES
USE_MDFILE_AS_MAINPAGE = @CMAKE_SOURCE_DIR@/README.md
STRIP_FROM_PATH = @CMAKE_SOURCE_DIR@
STRIP_FROM_INC_PATH = @CMAKE_SOURCE_DIR@
DOT_PATH = @DOXYGEN_DOT_EXECUTABLE@
HAVE_DOT = @DOXYGEN_DOT_FOUND@
CALL_GRAPH = YES
CALLER_GRAPH = YES
HIDE_UNDOC_RELATIONS = NO
HIDE_IN_BODY_DOCS = YES
EXCLUDE_PATTERNS = *test.cc */library/*/libp2p/*.*
FILE_PATTERNS = *.h *.cc *.md
PROJECT_NAME = ipfs-chromium
WARN_NO_PARAMDOC = YES
